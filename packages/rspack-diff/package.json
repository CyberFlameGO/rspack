{
  "name": "@rspack/diff",
  "version": "0.3.0",
  "license": "MIT",
  "description": "DiffTool for Rspack and Webpack",
  "main": "./src/index.js",
  "bin": {
    "rspack-diff": "./src/index.js"
  },
  "scripts": {
    "build": "node scripts/buildAll.js",
    "test": "node scripts/testAll.js",
    "diff:basic": "node . dist fixtures/basic/rspack-dist/main.js fixtures/basic/webpack-dist/main.js --no-dry",
    "diff:antd": "node . dist fixtures/antd/rspack-dist/main.js fixtures/antd/webpack-dist/main.js",
    "diff:three": "node . dist fixtures/three/rspack-dist/main.js fixtures/three/webpack-dist/main.js",
    "diff:lodash": "node . dist fixtures/lodash/rspack-dist/main.js fixtures/lodash/webpack-dist/main.js",
    "diff:lodash_stats": "node . stats fixtures/lodash/rspack-dist/stats.json fixtures/lodash/webpack-dist/stats.json"
  },
  "files": [
    "src"
  ],
  "homepage": "https://rspack.dev",
  "bugs": "https://github.com/web-infra-dev/rspack/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/rspack",
    "directory": "packages/rspack-diff"
  },
  "dependencies": {
    "acorn": "^8.0.4",
    "acorn-walk": "^8.0.0"
  },
  "devDependencies": {
    "@rspack/core": "workspace:*",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21",
    "three": "0.155.0",
    "antd": "5.8.4"
  }
}